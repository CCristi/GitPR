{
  "manifest_version": 2,
  "name": "Git merger",
  "description": "This extension created a merge commit in develop",
  "version": "1.0",
  "background": {
    "page": "index.html"
  },
  "browser_action": {},
  "options_page": "options.html",
  "permissions": [
    "tabs",
    "activeTab",
    "background",
    "storage",
    "https://ellation.atlassian.net/*"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
