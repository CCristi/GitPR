{
  "manifest_version": 2,
  "name": "Merge assistant",
  "description": "This extension creates merge commit for you",
  "version": "1.0",
  "background": {
    "page": "index.html"
  },
  "browser_action": {},
  "options_page": "options.html",
  "permissions": [
    "tabs",
    "activeTab",
    "background",
    "storage",
    "https://*.atlassian.net/*"
  ],
  "icons": {
    "16": "icon128.png",
    "48": "icon128.png",
    "128": "icon128.png"
  }
}
